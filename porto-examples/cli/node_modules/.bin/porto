#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/cli/bin/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/cli/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/cli/bin/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/cli/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/_dist/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules/porto/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/porto@0.2.19_@tanstack+react-query@5.90.2_react@19.1.0__@types+react@19.1.14_@wagmi+cor_d0e08fc63fddffb9ffbd9cc234b0ed29/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../porto/_dist/cli/bin/index.js" "$@"
else
  exec node  "$basedir/../porto/_dist/cli/bin/index.js" "$@"
fi
