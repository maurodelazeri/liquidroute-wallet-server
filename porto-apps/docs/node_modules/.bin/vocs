#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/_lib/cli/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/_lib/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/_lib/cli/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/_lib/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules/vocs/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/vocs@1.0.13_@types+node@22.18.6_@types+react-dom@19.1.9_@types+react@19.1.14__@types+re_2d34df5abdc34f709c8f35ab9ee4afac/node_modules:/Users/maurodelazeri/work/src/github.com/quiknode-labs/wallet/liquidroute-wallet-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vocs/_lib/cli/index.js" "$@"
else
  exec node  "$basedir/../vocs/_lib/cli/index.js" "$@"
fi
